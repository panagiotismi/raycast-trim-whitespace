{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "trim-whitespace",
  "title": "Whitespace Trim",
  "description": "Trims leading and trailing whitespace before pasting",
  "icon": "command-icon.png",
  "author": "panagiotismi",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Trim Whitespace",
      "subtitle": "Clipboard",
      "description": "Trims leading and trailing whitespace before pasting",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
